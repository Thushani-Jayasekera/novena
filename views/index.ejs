<script>
    let clientId="978225651454-3rp3tki3fef9rpd2ccocnt1fep7881m6.apps.googleusercontent.com";
    let redirectUri = "http://127.0.0.1:5500/connect";
    let scopes = "https://www.googleapis.com/auth/drive"
	let responseType = "token";
    
    let button = document.getElementById('drive-button')
    
    button.onclick = function() {
        console.log('clicked');
		let oauth2Endpoint = "https://accounts.google.com/o/oauth2/v2/auth";

		let form = document.createElement("form");
		form.setAttribute("method", "GET");
		form.setAttribute("action", oauth2Endpoint);


		let params = {
			client_id : clientId,
			redirect_uri: redirectUri,
			response_type : responseType,
			scope: scopes,
			api_key: "AIzaSyBSZf8DMZ8QkiaYJ3vfnolza_lGCvVYPck",
			include_granted_scopes : "true",
			state: "pass-through-value"
		}

		for (var p in params) {
			let input= document.createElement("input");
			input.setAttribute("type", "hidden");
			input.setAttribute("name", p);
			input.setAttribute("value", params[p]);
			form.appendChild(input);
		}

		document.body.appendChild(form);

		form.submit();
      };
</script>
